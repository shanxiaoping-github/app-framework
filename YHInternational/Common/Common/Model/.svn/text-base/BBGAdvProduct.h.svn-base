#import "BBGProduct.h"

@interface BBGAdvProduct : BBGProduct
/*!
 *  广告URL
 */
@property (nonatomic, strong) NSString * adURL;
/*!
 *  二维码
 */
@property (nonatomic, strong) NSString * barCode;
/*!
 *  广告图
 */
@property (nonatomic, strong) NSString * adImg;
/*!
 *  广告名称
 */
@property (nonatomic, strong) NSString * adName;
/*!
 *  1，标识轮播图，2标识专题
 */
@property (nonatomic, strong) NSString * type;
/*!
 *  id 美妆母婴
 */
@property (nonatomic, strong) NSString *charsId;
/*!
 *  pointType 美妆母婴
 */
@property (nonatomic, strong) NSString *charsPointType;
/**
 *  搜索关键字
 */
@property (nonatomic, strong) NSString *searchWord;
/**
 *  结束时间
 */
@property (nonatomic, strong) NSString *endTime;
/**
 *  开始时间
 */
@property (nonatomic, strong) NSString *beginTime;
/**
 *  货品id
 */
@property (nonatomic, strong) NSString *productId;

/**
 *  isNav 1标识开启，0 表示不开启
 */
@property (nonatomic, strong) NSString *isHaveNav;
@property (nonatomic, strong) NSString *activityId;

@end
