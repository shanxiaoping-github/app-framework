#import "BBGResponseDataHandler.h"

@interface BBGPromotionInfo : BBGResponseDataHandler
/**
 *  用户能购买的数量
 */
@property (nonatomic,assign) NSInteger canBuyNum;
/**
 *  活动配置的用户可购买的单品的最大数量
 */
@property (nonatomic,assign) NSInteger numPerMember;
/**
 *  用户已购买的单品的数量
 */
@property (nonatomic,assign) NSInteger realNum;
/**
 *  活动配置的单品限购库存类型 ：1：限制总数量；2：限制每天数量
 */
@property (nonatomic,assign) NSInteger limitType;
/**
 *  活动配置的库存限制阀值
 */
@property (nonatomic,assign) NSInteger limitValue;
/**
 *  已售出数量
 */
@property (nonatomic,assign) NSInteger realValue;
/**
 *  活动配置的用户每单可购买的货品的种类数
 */
@property (nonatomic,assign) NSInteger kindPerMemberOrder;

@end
