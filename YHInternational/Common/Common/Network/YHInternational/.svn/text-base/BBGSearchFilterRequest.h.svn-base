#import "BBGRequest.h"

@interface BBGSearchFilterRequest : BBGRequest

/**
 *  页码，默认为1
 */
@property (nonatomic, assign) NSUInteger page;
/**
 *  每页商品数，默认为20
 */
@property (nonatomic, assign) NSUInteger pageSize;
/**
 *  后台分类id（切换分类）
 */
@property (nonatomic, strong) NSString *backCate;
/**
 *  前台分类id,用于服务端查询上次的分类信息
 */
@property (nonatomic, strong) NSString *lastFCate;
/**
 *  搜索关键字,用于服务端查询上次的分类信息
 */
@property (nonatomic, strong) NSString *lastKWs;

@end
