#import "UIImage+Stretch.h"

@implementation UIImage (Stretch)

- (UIImage *)stretchImage {
    return [self resizableImageWithCapInsets:UIEdgeInsetsMake(self.size.width / 2.0, self.size.height / 2.0, self.size.width / 2.0 - 1, self.size.height / 2.0 -1) resizingMode:UIImageResizingModeStretch];
}

@end
